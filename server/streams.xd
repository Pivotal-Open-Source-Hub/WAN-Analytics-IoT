admin config server http://192.168.2.1:9393

stream create --name input --definition "http --port=9000 | gemfire-json-server --host=192.168.2.1 --port=10334 --useLocator=true --regionName=Probe_requests --keyExpression=payload.getField('nanoTimestamp')" --deploy

